#install.packages("cronR")
library(cronR)

f <- "/media/My_Files/Dropbox/Project_data/Code/test_2.R"
#f <- "/media/My_Files/Dropbox/Project_data/Data_scraping_v.1.0.2.R"
cmd <- cron_rscript(f)
#cmd
  
cron_clear(ask=FALSE)
cron_add(command = cmd, frequency = 'hourly', at='15:09', id = 'main_job')
cron_njobs()
cron_ls()

